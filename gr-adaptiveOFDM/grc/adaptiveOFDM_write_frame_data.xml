<?xml version="1.0"?>
<block>
  <name>Write Frame Data</name>
  <key>adaptiveOFDM_write_frame_data</key>
  <category>[Time Adaptive OFDM]</category>
  <import>import adaptiveOFDM</import>
  <make>adaptiveOFDM.write_frame_data($snr_file, $snr_var_file, $enc_file, $delay_file, $debug)</make>

  <param>
    <name>SNR File</name>
    <key>snr_file</key>
    <value>/tmp/snr.csv</value>
    <type>string</type>
  </param>

  <param>
    <name>SNR var File</name>
    <key>snr_var_file</key>
    <value>/tmp/snr_var.csv</value>
    <type>string</type>
  </param>

  <param>
    <name>Encoding File</name>
    <key>enc_file</key>
    <value>/tmp/encoding.csv</value>
    <type>string</type>
  </param>

  <param>
    <name>Frame Delay File</name>
    <key>delay_file</key>
    <value>/tmp/wifi_frame_delay.csv</value>
    <type>string</type>
  </param>

  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>True</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>

  <sink>
    <name>frame data</name>
    <type>message</type>
  </sink>
</block>
